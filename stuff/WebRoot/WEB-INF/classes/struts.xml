<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="loginPackage" extends="struts-default">
		<action name="login" class="com.lgh.action.LoginAction" method="login">
			<result name="success">loginSuccess.jsp</result>
			<result name="error">loginFail.jsp</result>
		</action>
		<action name="register" class="com.lgh.action.RegisterAction" method="login">
			<result name="success">query.jsp</result>
			<result name="error">register.jsp</result>
		</action>
	</package>
</struts>